/****** Main styles for image wall ******/

.search-overlay {
	position: fixed;
	z-index: 1;
	width: 100%;
	height: 100%;
	background: rgba(255, 105, 180, .85);
}

.search-wrapper {
	text-align: center;
}

.search-overlay input[type=text] {
	font-family: 'Courgette', cursive;
	width: 50%;
	background: none;
	height: 170px;
	border: none;
	font-size: 115px;
	text-align: center;
	margin-top: 300px;
	/* position: relative; */
	color: white;
	outline-offset: 0;
	outline: none;
}

.search-overlay input[type=submit] {
	opacity: 0;
}

.fadeOut {
  -webkit-opacity: 0;
  -moz-opacity: 0;
  opacity: 0;
  -webkit-transition: all .3s ease;
  -moz-transition: all .3s ease;
  -ms-transition: all .3s ease;
  -o-transition: all .3s ease;
  transition: all .3s ease;
  z-index: -1;
}

.fadeIn {
	display: inherit;
	-webkit-opacity: 1;
	-moz-opacity: 1;
	opacity: 1;
	-webkit-transition: all .8s ease;
	-moz-transition: all .8s ease;
	-ms-transition: all .8s ease;
	-o-transition: all .8s ease;
	transition: all .8s ease;
}

.heart-overlay {
	width: 130px;
	height: 130px;
	background: url(/assets/big-heart.png);
	z-index: 2;
	position: absolute;
	margin-left: 105px;
	margin-top: 115px;
}







